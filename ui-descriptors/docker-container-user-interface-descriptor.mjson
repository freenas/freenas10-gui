{
    "root": {
        "prototype": "montage/core/meta/user-interface-descriptor",
        "properties": {
            "inspectorComponentModule": {"%": "ui/sections/containers/inspectors/container.reel"},
            "nameExpression": "id.defined() ? names.0 : 'Choose a collection'",
            "collectionNameExpression": "'Containers'",
            "creatorComponentModule": {"%": "ui/sections/containers/controls/docker-collection-list.reel"},
            "statusColorMapping": {
                "running": "green",
                "stopped": "grey"
            },
            "statusValueExpression": "running ? 'running' : 'stopped'",
            "daoModuleId": "core/dao/docker-container-dao"
        }
    }
}
