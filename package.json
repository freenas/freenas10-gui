    "term.js": "0.0.7",
    "validators": "~0.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "commander": "~2.9.0",
    "json.sortify": "~1.0.2",
    "mocha": "^2.5.3",
    "montage-testing": "~0.4.0",
